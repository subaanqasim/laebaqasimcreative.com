{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "types": ["astro/client-image"],
    "exactOptionalPropertyTypes": false,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@lib/*": ["src/lib/*"],
      "@public/*": ["public/*"],
      "@assets/*": ["src/assets/*"]
    },
    "jsx": "preserve"
  }
}
