{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@lib/*": ["src/lib/*"],
      "@public/*": ["public/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
